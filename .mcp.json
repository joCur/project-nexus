{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {}
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "semgrep": {
      "type": "http",
      "url": "https://mcp.semgrep.ai/mcp"
    },
    "linear-server": {
      "command": "npx",
      "args": ["-y", "@tacticlaunch/mcp-linear"]
    }
  }
}
